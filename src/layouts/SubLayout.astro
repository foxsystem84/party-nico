---
import SEO from "../components/SEO.astro";
interface Props {
  title: string;
  description: string;
}

const { title, description } = Astro.props;
---

<!doctype html>
<html lang="es">
  <head>
    <SEO {title} {description} />
  </head>
  <body>
    <header>
      <img
        src="/images/banner.webp"
        alt=""
        width="1920px"
        height="300px"
        class=""
      />
    </header>
    <slot />
  </body>
</html>
